    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "External terminal",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
          "run",
          "start:dev"
        ],
        "console": "externalTerminal",
        "restart": true,
        // "protocol": "auto",
        "autoAttachChildProcesses": true
      },
      {
        "type": "chrome",
        "name": "https://localhost:3003",
        "request": "launch",
        "url": "https://localhost:3003"
      }
    ]
  }
  

